<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <section id="retailplace_attribute_updater">
            <group id="closed_until" translate="label" type="text" sortOrder="20" showInDefault="1" showInWebsite="1" showInStore="1">
                <label>Closed Until</label>
                <field id="cron_schedule" translate="label" type="text" sortOrder="10" showInDefault="1"
                       showInWebsite="1" showInStore="1">
                    <label>Cron Schedule</label>
                </field>
            </group>
            <group id="has_new_products" translate="label" type="text" sortOrder="30" showInDefault="1"
                   showInWebsite="1" showInStore="1" canRestore="1">
                <field id="has_new_products_label" translate="label" type="text" sortOrder="20" showInDefault="1"
                       showInWebsite="1" showInStore="1" canRestore="1">
                    <label>Frontend label</label>
                </field>
            </group>
        </section>
    </system>
</config>
