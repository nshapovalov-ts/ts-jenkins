<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <section id="aghaseller" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label">
            <label>Agha Sellers</label>
            <tab>vdcstore</tab>
            <resource>Retailplace_Aghaseller::config_retailplace_aghaseller</resource>
            <group id="aghaseller" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label">
                <label>Agha Sellers Configuration</label>
                <field id="enable" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label" type="select">
                    <label>Enable</label>
                    <comment/>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="per_page_values" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="20" translate="label" type="text">
                    <label>Sellers per Page Allowed Values</label>
                    <comment/>
                </field>
                <field id="limit" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="30" translate="label" type="text">
                    <label>Sellers per Page Default Value</label>
                    <comment/>
                </field>
            </group>
        </section>
    </system>
</config>
