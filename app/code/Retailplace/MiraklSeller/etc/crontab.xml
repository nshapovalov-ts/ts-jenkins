<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="mirakl_seller_accept_all_orders" instance="MiraklSeller\Sales\Cron\AcceptAllOrders" method="execute">
            <config_path>crontab/default/jobs/mirakl_seller_accept_all_orders/schedule/cron_expr</config_path>
        </job>
        <job name="mirakl_seller_sync_all_orders" instance="MiraklSeller\Sales\Cron\SyncAllOrders" method="execute">
            <config_path>crontab/default/jobs/mirakl_seller_sync_all_orders/schedule/cron_expr</config_path>
        </job>
    </group>
</config>
